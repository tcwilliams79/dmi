{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://dmianalysis.org/schemas/weights_vintage_review_packet.schema.json",
  "title": "DMI Weights Vintage Review Packet",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "schema_version": {
      "type": "string"
    },
    "run_id": {
      "type": "string"
    },
    "reference_period": {
      "type": "string",
      "pattern": "^[0-9]{4}-[0-9]{2}$"
    },
    "created_at": {
      "type": "string",
      "format": "date-time"
    },
    "old_weights_year": {
      "type": [
        "integer",
        "null"
      ]
    },
    "new_weights_year": {
      "type": "integer"
    },
    "gate_status": {
      "type": "string",
      "enum": [
        "TRIGGERED",
        "NOT_TRIGGERED"
      ]
    },
    "publish_decision_default": {
      "type": "string",
      "enum": [
        "FAIL_PUBLISH_BY_DEFAULT",
        "ALLOW_PUBLISH",
        "ALLOW_PUBLISH_WITH_WARNING"
      ]
    },
    "candidate_backfill": {
      "type": [
        "object",
        "null"
      ],
      "additionalProperties": true,
      "description": "Optional: pointers to an internally-generated candidate backfill release computed with the new weights."
    },
    "diff_summary": {
      "type": [
        "object",
        "null"
      ],
      "additionalProperties": true,
      "description": "Optional: summary diffs (e.g., per quintile level shifts, dispersion changes) between prior published release and candidate."
    },
    "notes": {
      "type": [
        "string",
        "null"
      ]
    }
  },
  "required": [
    "schema_version",
    "run_id",
    "reference_period",
    "created_at",
    "new_weights_year",
    "gate_status",
    "publish_decision_default"
  ]
}